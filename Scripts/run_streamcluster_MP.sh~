#!/bin/bash

source ./scripts/paths.sh

echo " "
echo "---Running Stream Culster OpenMP on simulator---"
for i in 1 2 4
do
$m2s_cgm_path/m2s-cgm --x86-sim detailed --x86-config $config_file_path/$cpu_config_file_name $benchmark_path/OpenMP/streamcluster/sc_omp 10 20 8 16 16 20 none $benchmark_path/output $i
done




/home/stardica/Desktop/m2s-cgm/Release/m2s-cgm --x86-sim detailed --x86-config /home/stardica/Desktop/m2s-cgm/src/config/Intel-i7-4790k-CPU-Config.ini --si-sim detailed --si-config /home/stardica/Desktop/m2s-cgm/src/config/Radeon-HD-7870-GPU-Config.ini /home/stardica/Dropbox/WorkingRodiniaBenchmarks/OpenMP/streamcluster/sc_omp 10 20 8 16 16 20 none output_p2 2

/home/stardica/Desktop/m2s-cgm/Release/m2s-cgm --x86-sim detailed --x86-config /home/stardica/Desktop/m2s-cgm/src/config/Intel-i7-4790k-CPU-Config.ini --si-sim detailed --si-config /home/stardica/Desktop/m2s-cgm/src/config/Radeon-HD-7870-GPU-Config.ini /home/stardica/Dropbox/WorkingRodiniaBenchmarks/OpenMP/streamcluster/sc_omp 10 20 8 16 16 20 none output_p4 4

# --mem-config /home/stardica/Desktop/m2s-cgm/src/config/cgm-config.ini
